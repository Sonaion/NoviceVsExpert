    "source-code": [
        "    static int binaryToDecimal(String str, int position) {",
        "        if (position < 0) {",
        "            return 0;",
        "        } else if (str.charAt(position) == '0') {",
        "            return 2 * binaryToDecimal(str, position - 1);",
        "        }",
        "",
        "        return 1 + 2 * binaryToDecimal(str, position - 1);",
        "    }"
    ]
}