    "source-code": [
        "    public static int hIndex(int[] numbers) {",
        "        Arrays.sort(numbers);",
        "",
        "        int count = numbers.length;",
        "        int hIndex = 0;",
        "        for (int i = count - 1; i >= 0; i--) {",
        "            int current = count - 1;",
        "            if (numbers[i] >= current) {",
        "                hIndex = current;",
        "            } else {",
        "                break;",
        "            }",
        "        }",
        "        return hIndex;",
        "    }"
    ]
}