    "source-code": [
        "    public static int[] dropNumber(int[] input, int pos) {",
        "        if (input.length == 0 || pos >= input.length) {",
        "            throw new IllegalArgumentException();",
        "        }",
        "",
        "        int[] result = new int[input.length - 1];",
        "",
        "        for (int i = 0; i < pos; i++) {",
        "            result[i] = input[i];",
        "        }",
        "",
        "        for (int i = pos; i + 1 < input.length; i++) {",
        "            result[i] = input[i + 1];",
        "        }",
        "",
        "        return result;",
        "    }"
    ]
}