    "source-code": [
        "    public static float arrayAverage(int[] numbers) {",
        "        int count = 0;",
        "        int sum = 0;",
        "",
        "        while (count < numbers.length) {",
        "            sum = sum + numbers[count];",
        "            count = count + 1;",
        "        }",
        "",
        "        float average = sum / (float) count;",
        "        return average;",
        "    }"
    ]
}