    "source-code": [
        "    public static void reverse(int[] array) {",
        "        for (int i = 0; i <= array.length / 2 - 1; i++) {",
        "            int tmp = array[array.length - 1 - i];",
        "            array[array.length - i - 1] = array[i];",
        "            array[i] = tmp;",
        "        }",
        "    }"
    ]
