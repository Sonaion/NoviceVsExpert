    "source-code": [
        "    public static int ackermann(int n, int m) {",
        "        if (n == 0) {",
        "            return m + 1",
        "        } else if (m == 0) {",
        "            return ackermann(n - 1, 1);",
        "        }",
        "",
        "        return ackermann(n - 1, ackermann(n, m - 1);",
        "    }"
    ]
}